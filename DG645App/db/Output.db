record(ao, "$(P)$(R)$(N)LAMP")
{
    field(DESC, "Level Amplitude")
    field(DTYP, "stream")
    field(OUT,  "@DG645.proto setDoutput(LAMP,$(ID)) $(PORT) $(A)")
    field(FLNK, "$(P)$(R)$(N)LAMP_RBV")
}

record(ai, "$(P)$(R)$(N)LAMP_RBV")
{
    field(DESC, "Level Amplitude")
    field(DTYP, "stream")
    field(INP,  "@DG645.proto getDoutput(LAMP,$(ID)) $(PORT) $(A)")
    field(PINI, "YES")
}

