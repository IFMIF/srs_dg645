record(ai, "$(P)$(R)$(N)Delay_RBV")
{
    field(DESC, "Delay string")
    field(DTYP, "stream")
    field(INP,  "@DG645.proto getDelay($(P)$(R)$(N),$(ID)) $(PORT) $(A)")
    field(PINI, "YES")
    field(SCAN, "1 second")
    field (PREC, "12")

}

